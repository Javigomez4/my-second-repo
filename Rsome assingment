      "cell_type": "code",
      "source": [
        "\n",
        "from rsome import ro\n",
        "import rsome as rso\n",
        "import math\n",
        "import numpy as np\n",
        "\n",
        "n = 150\n",
        "\n",
        "h = [0.15 + (i * 0.05) / 150.0 for i in range(1, n+1)]\n",
        "a = [(0.05 / 450.0) * math.sqrt(2.0 * i * n * (n + 1)) for i in range(1, n+1)]\n",
        "\n",
        "b_val = 0.03\n",
        "Gamma = 4.0\n",
        "\n",
        "model = ro.Model('RobustProblem')\n",
        "\n",
        "x = model.dvar(n)    # decision variables\n",
        "z = model.rvar(n)    # uncertain vector\n",
        "\n",
        "# Uncertainty set Z(Gamma): |z|_inf <= 1 and ||z||_1 <= Gamma\n",
        "zset = (rso.norm(z, 'inf') <= 1, rso.norm(z, 1) <= Gamma)\n",
        "\n",
        "# Objective: maximize h^T x\n",
        "model.max(sum(h[i] * x[i] for i in range(n)))\n",
        "\n",
        "# Robust constraint: (a + z)^T x <= b  for all z in Z(Gamma)\n",
        "model.st(((sum((a[i] + z[i]) * x[i] for i in range(n))) <= b_val).forall(zset))\n",
        "\n",
        "# Bounds on x\n",
        "model.st(x >= 0)\n",
        "model.st(x <= 1)\n",
        "model.st(sum(x[i] for i in range(n)) <= 1)\n",
        "\n",
        "# Solve\n",
        "model.solve()\n",
        "\n",
        "print(\"Optimal objective value:\", model.get())\n",
        "x_vals = x.get()\n",
        "print(\"Nonzero x components:\")\n",
        "for i, val in enumerate(x_vals):\n",
        "    if val > 1e-6:\n",
        "        print(f\"x[{i+1}] = {val:.6f}\")\n",
        "\n"
      ],
